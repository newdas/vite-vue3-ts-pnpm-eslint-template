{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "@vue/typescript/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "plugins": [
  ],
  "rules": {
    "no-multiple-empty-lines": [1, {"max": 1, "maxEOF": 1}],
    "semi": [2, "never"],
    "no-console": "error"
  }
}
